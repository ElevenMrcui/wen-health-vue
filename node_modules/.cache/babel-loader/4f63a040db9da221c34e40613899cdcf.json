A,CAAA,YAHA;AAIA,cAAA,IAAA,EAAA,MAJA;AAKA,cAAA,IAAA,EAAA;AALA,aAAA;AAnBA,WAAA;AA2BA,SA9BA,MA8BA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA;AAEA,OAnCA;AAoCA;AAtCA,GAVA;AAkDA,EAAA,OAlDA,qBAkDA;AACA,QAAA,QAAA,GAAA,IAAA,IAAA,EAAA;AACA,QAAA,QAAA,GAAA,QAAA,CAAA,WAAA,KAAA,CAAA;AACA,QAAA,KAAA,GAAA,QAAA,CAAA,QAAA,KAAA,CAAA;AACA,QAAA,IAAA,GAAA,QAAA,CAAA,OAAA,EAAA;AACA,SAAA,IAAA,CAAA,KAAA,GAAA,QAAA,GAAA,GAAA,GAAA,KAAA,GAAA,GAAA,GAAA,IAAA;AACA,SAAA,IAAA,CAAA,GAAA,GAAA,QAAA;AACA,SAAA,QAAA;AACA;AA1DA,CAAA","sourcesContent":["<template>\r\n<div class=\"hold-transition\">\r\n  <div class=\"content-header\">\r\n                <h1>统计分析<small>会员数量</small></h1>\r\n                <el-breadcrumb separator-class=\"el-icon-arrow-right\" class=\"breadcrumb\">\r\n                    <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n                    <el-breadcrumb-item>统计分析</el-breadcrumb-item>\r\n                    <el-breadcrumb-item>会员数量</el-breadcrumb-item>\r\n                </el-breadcrumb> <br><br>\r\n\r\n   <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n           \r\n      <!-- <span class=\"demonstration\">默认</span> -->\r\n    <el-date-picker\r\n      v-model=\"date.begin\"\r\n      type=\"date\"\r\n      placeholder=\"开始日期\">\r\n    </el-date-picker>\r\n    \r\n    <el-date-picker\r\n      v-model=\"date.end\"\r\n      type=\"date\"\r\n      placeholder=\"结尾日期\">\r\n    </el-date-picker>\r\n    <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onSubmit\">查询</el-button>\r\n    </el-form-item> \r\n  </el-form>\r\n\r\n   \r\n        </div>\r\n\r\n            <div class=\"app-container\">\r\n                <div class=\"box\">\r\n                    <!-- 为 ECharts 准备一个具备大小（宽高）的 DOM -->\r\n                    <div id=\"chart1\" style=\"height:600px;\"></div>\r\n                </div>\r\n            </div>      \r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            date:{\r\n                begin:'',\r\n                end:''\r\n            }\r\n        }\r\n    },\r\n           \r\n            methods: {\r\n                onSubmit() {\r\n                   this.$http.post('api/report/memberReportByLastYear',this.date).then((res)=>{\r\n                     if(res.data.flag){\r\n                      var myChart = this.$echarts.init(document.getElementById(\"chart1\"));\r\n\r\n                         myChart.setOption({\r\n                             title:{\r\n                                 text:'会员数量'\r\n                             },\r\n                             tooltip: {\r\n                                 trigger: 'axis',\r\n                                 formatter: res.data.data.memberCounts\r\n                                 //数量\r\n                                 //data:res.data.data.memberCount\r\n                            },\r\n                             xAxis: {\r\n                                 type: 'category',\r\n                                 data: res.data.data.monthList\r\n                               //日期\r\n                               //  data: res.data.data.monthList\r\n                             },\r\n                             yAxis: {\r\n                                 type: 'value'\r\n                             },\r\n                             series: [{\r\n                                //数量\r\n                                 //data:res.data.data.memberCount\r\n                                 data: res.data.data.memberCounts,\r\n                                 name:'会员数量',\r\n                                 type: 'line'\r\n                             }]\r\n                         });\r\n                     }else{\r\n                         this.$message.error=res.data.message\r\n                     }\r\n\r\n               })\r\n                }    \r\n            },\r\n            created(){\r\n                var dateTime = new Date();\r\n                var lastYear = dateTime.getFullYear()-1;\r\n                var month = dateTime.getMonth()+1;\r\n                var date = dateTime.getDate()\r\n                this.date.begin=lastYear+'-'+month+'-'+date\r\n                this.date.end= dateTime\r\n                this.onSubmit()\r\n            },\r\n};\r\n</script>"],"sourceRoot":"src/views/member"}]}