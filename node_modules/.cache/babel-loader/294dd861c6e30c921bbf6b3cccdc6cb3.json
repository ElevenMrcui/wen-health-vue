,KAnBA;AAqBA;AACA,IAAA,qBAtBA,iCAsBA,GAtBA,EAsBA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA,KAxBA;AA0BA;AACA,IAAA,QA3BA,sBA2BA;AAAA;;AACA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,+BAAA,EAAA,KAAA,UAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,UAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA,OAPA;AAQA,KArCA;AAuCA;AACA,IAAA,mBAxCA,+BAwCA,OAxCA,EAwCA;AACA,WAAA,UAAA,CAAA,WAAA,GAAA,OAAA;AACA,WAAA,QAAA;AACA,KA3CA;AA6CA;AACA,IAAA,QA9CA,oBA8CA,GA9CA,EA8CA;AAAA;;AACA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,4BAAA,GAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,UAAA,CAAA,WAAA,GAAA,CAAA;;AACA,UAAA,MAAA,CAAA,QAAA;AACA,SAJA,MAIA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA,OARA;AASA;AAzDA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"content-header\">\r\n      <h1>检查项历史数据<small>恢复数据</small></h1>\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\" class=\"breadcrumb\">\r\n        <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item>恢复数据</el-breadcrumb-item>\r\n        <el-breadcrumb-item>检查项历史数据</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n\r\n    <div class=\"app-container\">\r\n    <div class=\"box\">\r\n    <div class=\"filter-container\">\r\n\r\n    <div>\r\n        \r\n        <el-table ref=\"table\" size=\"small\" current-row-key=\"id\"  tooltip-effect=\"dark\" @selection-change=\"handleSelectionChange\"  :data=\"dataList\" stripe highlight-current-row>\r\n            \r\n            <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n\r\n            <el-table-column type=\"index\" align=\"center\" label=\"序号\"></el-table-column>\r\n          \r\n            <el-table-column prop=\"code\" label=\"项目编码\" align=\"center\" ></el-table-column>\r\n\r\n            <el-table-column prop=\"name\" label=\"项目名称\" align=\"center\"></el-table-column>  \r\n\r\n            <el-table-column label=\"适用性别\" align=\"center\">\r\n                <template slot-scope=\"scope\">\r\n                    <span>{{scope.row.sex == \"0\" ? \"不限\" : scope.row.sex == \"1\" ? \"男\" : \"女\" }}</span>\r\n                </template>\r\n            </el-table-column> \r\n\r\n            <el-table-column prop=\"age\" label=\"适用年龄\" align=\"center\"></el-table-column>\r\n          \r\n            <el-table-column prop=\"remark\" label=\"项目说明\" align=\"center\" ></el-table-column>\r\n\r\n            <el-table-column label=\"操作\" align=\"center\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button type=\"primary\" size=\"mini\" @click=\"recovery(scope.row)\" >恢复</el-button >\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n    </div>\r\n\r\n\r\n    <div class=\"pagination-container\">\r\n        <div style=\"margin-top: 20px\" >\r\n            <el-button type=\"primary\" @click=\"restoreAll\">恢复选中数据</el-button>\r\n            <el-button type=\"primary\" @click=\"toggleSelection\">取消选择</el-button>\r\n        </div>\r\n\r\n      <el-pagination\r\n        class=\"pagiantion\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pagination.currentPage\"\r\n        :page-size=\"pagination.pageSize\"\r\n        layout=\"total, prev, pager, next, jumper\"\r\n        :total=\"pagination.total\">\r\n      </el-pagination>\r\n    </div>\r\n    </div>\r\n    </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n    \r\n        pagination: {\r\n            //分页相关模型数据\r\n            currentPage: 1, //当前页码\r\n            pageSize: 5, //每页显示的记录数\r\n            total: 0, //总记录数\r\n            queryString: null, //查询条件   双向绑定\r\n        },\r\n        dataList: [], //当前页要展示的分页列表数据  [{},{}]  List<T>\r\n        formData: {}, //表单数据  添加检查项  formData提交给后台\r\n        multipleSelection: []\r\n    };\r\n  },\r\n\r\n  //钩子函数，VUE对象初始化完成后自动执行\r\n  created() {\r\n    this.findPage();\r\n  },\r\n\r\n  methods: {\r\n\r\n      //点击按钮恢复选中数据\r\n      restoreAll(){\r\n        //alert(JSON.stringify(this.multipleSelection))\r\n        this.$http.post(\"api/checkitem/restoreAll\",this.multipleSelection).then((res)=>{\r\n            if(res.data.flag){\r\n                this.$message.success(\"已恢复所有数据\")\r\n                this.pagination.currentPage = 1;\r\n                this.findPage()\r\n            }else{\r\n                this.$message.error(res.data.message)\r\n            }\r\n        })\r\n      },\r\n\r\n      //点击取消按钮时取消所有选中\r\n      toggleSelection() {\r\n          this.$refs.table.clearSelection(); \r\n      },\r\n\r\n      //点击复选框是multipleSelection数组元素+1   \r\n      handleSelectionChange(val) {\r\n        this.multipleSelection = val;\r\n      },\r\n\r\n    // 分页查询\r\n    findPage() {\r\n      //  第一次分页查询\r\n      this.$http.post(\"api/checkitem/findDeletedPage\", this.pagination).then((res) => {\r\n        if (res.data.flag) {\r\n          this.dataList = res.data.data.rows;\r\n          this.pagination.total = res.data.data.total;\r\n        } else {\r\n          this.$message.error(res.data.message);\r\n        }\r\n      });\r\n    },\r\n\r\n     //点击页码进行分页查询\r\n    handleCurrentChange(curpage) {\r\n      this.pagination.currentPage = curpage;\r\n      this.findPage();\r\n    },\r\n\r\n    //恢复数据\r\n    recovery(row){\r\n        //alert(JSON.stringify(row))\r\n        this.$http.get(\"api/checkitem/recovery/\"+row.id).then((res) => {\r\n            if(res.data.flag){\r\n                this.$message.success(\"恢复成功\")\r\n                this.pagination.currentPage = 1;\r\n                this.findPage()\r\n            }else{\r\n                this.$message.error(res.data.message)\r\n            }\r\n        })\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views/appoment"}]}